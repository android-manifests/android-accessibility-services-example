# Android Studio 项目的 .gitignore

# 个人配置
.idea/workspace.xml
.idea/tasks.xml
.idea/runConfigurations/
.idea/libraries/
.idea/dictionaries/
.idea/codeStyles/
.idea/caches/
.idea/dataSources/
.idea/dataSources.ids
.idea/sqlDataSources.xml
.idea/dynamic.xml
.idea/assetWizardSettings.xml
.idea

